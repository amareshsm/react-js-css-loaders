{
  "name": "react-spinner-css",
  "version": "1.0.0",
  "description": "React Spinner CSS",
  "main": "index.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "rollup -c",
    "format": "eslint **/*.js  && prettier **/*.js",
    "format:fix": "eslint **/*.js --fix && prettier **/*.js --write"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run format:fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/amareshsm/react-spinner-css.git"
  },
  "keywords": [
    "react",
    "css",
    "spinners"
  ],
  "author": "AMARESH S M ",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/amareshsm/react-spinner-css/issues"
  },
  "homepage": "https://github.com/amareshsm/react-spinner-css#readme",
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "@rollup/plugin-babel": "^5.1.0",
    "eslint": "^7.5.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "rimraf": "^3.0.2",
    "rollup": "^2.22.2",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-string": "^3.0.0",
    "rollup-plugin-terser": "^6.1.0"
  },
  "dependencies": {
    "prettier": "^2.0.5"
  },
  "peerDependencies": {
    "react": ">=16.2"
  },
  "engines": {
    "node": ">=8"
  }
}